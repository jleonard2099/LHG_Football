     IF K2(I,3,17)>E%(10) THEN GOSUB 7100:GOTO 8633
     IF K2(I,7,17)>E%(10) THEN GOSUB 7100:GOTO 8633
     NEXT
8633 GOTO 8635
7100 BEEP:Spec$ = DV$(1) + "*.SPC"
     Count = FCount%(Spec$)
     COUNT  = COUNT + 1
     SPECIAL% = 1
   RETURN
8635 REM
     END SUB

SUB SELECTTEAM ()
     SELECTION% = 1
     Count% = NT
     Start% = 1
     FileColr% = 79
     HiLiteColr% = 112
     Rows% = 20
     Columns% = 4
     Spacing% = 6
     Row% = 4
     Column% = 1
     COLOR , 4: CLS : LOCATE , 33: COLOR 14
     PRINT "TEAM SELECTION": COLOR 15
     LOCATE 25, 1: COLOR 14: PRINT "ESC"; : COLOR 15
     PRINT "-SELECT NEW ID  PGDN  PGUP  "; : COLOR 14
     PRINT "ENTER"; : COLOR 15: PRINT "-SELECTS TEAM";
     CALL SHOWCURSOR
20   MAMenu VARPTR(V$(1)), SELECTION%, Start%, Count%, EXITCODE%, FileColr%, HiLiteColr%, Rows%, Columns%, Spacing%, Row%, Column%
     CALL HIDECURSOR
     IF EXITCODE% = 27 THEN BO% = 1: GOTO 6102
     IF EXITCODE% = -71 THEN START% = 1:EXITCODE% = 3:SELECTION% = 1:GOTO 20
     IF EXITCODE% = -79 THEN START% = NT:EXITCODE% = 3:SELECTION% = NT:GOTO 20
     IF EXITCODE% = -73 THEN START% = START% - 80:IF START% < 1 THEN START% = 1
     IF EXITCODE% = -81 THEN START% = START% + 80:IF START% > NT THEN START% = NT
     IF EXITCODE% = -81 OR EXITCODE% = -73 THEN EXITCODE% = 3:SELECTION% = START%:GOTO 20
     B8% = V%(SELECTION%)
     LOCATE 2, 33: COLOR 31: PRINT "LOADING..."
     CALL GETTEAM
6102 REM
END SUB

SUB KICKSET ()
    U$=A7$(P1,Q7%(P1))+" IS PLACING THE BALL ON THE TEE":CALL PBP
    RN = INT(RND(1)*100)+1
    IF RN<=50 THEN
       U$=A$(P1)+" IS LINING UP FOR THE KICKOFF"
    ELSE
       U$="THE "+AA$(P1)+" ARE LINING UP FOR THE KICKOFF"
    END IF
    CALL PBP
    U$="AND THE KICK IS ON ITS WAY":CALL PBP:SWAP P1,D
    RN=INT(RND(1)*100)+1
     rem:*** added +y8% to the next 2 lines ***
    IF RN<=88 THEN N=100-(Y+INT(RND(1)*13)+55+Y8%)
    IF RN>88 THEN N=100-(Y+INT(RND(1)*8)+68+Y8%)
    IF G9=0 AND Y9=0 THEN N = N + 10
    IF WX%(0)=P1 THEN N=N-INT(RND(1)*7)+3-WX%(1)
    IF WX%(0)<>P1 THEN N=N+INT(RND(1)*7)-3+WX%(1)
END SUB

