SUB FINDTEAMS ()
     NT = 0
     OPEN DV$(0)+"FOOT."+YN$ AS #1 LEN=934
     Y = LOF(1)
     FIELD #1,15 AS Q$(0),640 AS X$
     FOR I1 = 1 TO Y / 934
     GET #1, I1
     FOR I2 = 1 TO 15
     IF ASC(MID$(Q$(0),I2,1))<32 OR ASC(MID$(Q$(0),I2,1))>123 OR UCASE$(LEFT$(Q$(0),3))="NOT" OR UCASE$(LEFT$(Q$(0),3))="UN" THEN 6105
     NEXT I2
     NT=NT+1
     V$(NT)=Q$(0):V%(NT)=I1
6105 NEXT I1
     CLOSE 1
     CALL SELECTTEAM
END SUB


