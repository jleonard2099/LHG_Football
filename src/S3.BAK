SUB CLIPPING ()
    U$="*** PENALTY - 10 YD ILLEGAL BLOCK ON THE RETURN ***"
    CALL PBP
    IF Y<20 THEN Y=Y/2:K(P1,12)=K(P1,12)+Y/2
    K(P1,11)=K(P1,11)+1:IF Y>=20 THEN Y=Y-10:K(P1,12)=K(P1,12)+10
END SUB

