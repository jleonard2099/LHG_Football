     U$="THE OFFICIAL SIGNALS FIRST DOWN!"
3427 CALL PBP:CALL PBP
END SUB

SUB WHICHRECEIVER
     IF I3>9 THEN U$=U$+A2$(P1,I3-10) ELSE U$=U$+A1$(P1,I3)
